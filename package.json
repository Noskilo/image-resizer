{
  "name": "image-resizer",
  "version": "1.0.0",
  "description": "Resizes an image to varying widths before committing into a storage bucket",
  "scripts": {
    "prepare": "npm run build",
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc -p .",
    "test": "jest --coverage --setupFiles dotenv/config",
    "create-env": "printenv > .env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Noskilo/image-resizer.git"
  },
  "author": "Michael Iskandarani",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Noskilo/image-resizer/issues"
  },
  "homepage": "https://github.com/Noskilo/image-resizer#readme",
  "dependencies": {
    "@google-cloud/storage": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "multer": "^1.4.2",
    "sharp": "^0.25.3",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^25.2.3",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.5",
    "@types/sharp": "^0.25.0",
    "@types/supertest": "^2.0.9",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
